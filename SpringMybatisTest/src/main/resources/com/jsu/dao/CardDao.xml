<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jsu.dao.CardMapper">

    <select id="getCardById" parameterType="int" resultType="com.jsu.bean.Card" >
        select id,card FROM card where id = #{id}
    </select>

</mapper>